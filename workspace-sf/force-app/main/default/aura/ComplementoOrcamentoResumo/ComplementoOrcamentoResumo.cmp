<aura:component description="ComplementoOrcamentoResumo" controller="ComplementoOrcamentoController" >

  <aura:handler name="change" action="{!c.doInit}" value="{!v.oportunidade}" />
  <aura:attribute name="oportunidade" type="Opportunity"  default="{ 'sobjectType': 'Opportunity' }" />
  <aura:attribute name="ehEditavel" type="Boolean" default="{! v.oportunidade.StageName == 'Em criação' }" />
  <!--<aura:attribute name="calculoTO" type="Object" />-->
  <aura:attribute name="precificacao" type="Object" />
  <aura:attribute name="analiseAtuarial" type="Object" />

  <aura:attribute name="listGarantiasParaAtualizar" type="SObject[]" />

  <aura:attribute name="formatoTaxa" type="String" default="0.00000" />
  <aura:attribute name="formatoMoeda" type="String" default="0.00" />
  <aura:attribute name="formatoPorcentagem" type="String" default="0.00" />

  <aura:attribute name="justificativaObrigatoria" type="String" default="{!$Label.c.JustificativaObrigatoria}" />

  <aura:method name="salvarAbaComplementos" action="{!c.salvarDescontoAgravo}" />
  <aura:method name="inicializarAbaComplementos" action="{!c.doInit}" />

  <lightning:spinner aura:id="spinner" alternativeText="Loading" size="large" class="slds-hide" />

  <!-- toast success -->
  <div class="demo-only slds-hide" aura:id="toastSuccess" style="height: 4rem; position: fixed; width: 100%; z-index: 10000; top: 67px;">
    <div class="slds-notify_container slds-is-relative">
      <div class="slds-notify slds-notify_toast slds-theme--success" role="alert">
        <span class="slds-assistive-text">success</span>
        <span class="slds-icon_container slds-icon-utility-info slds-m-right_small slds-no-flex slds-align-top" title="Description of icon when needed">
        <lightning:icon variant="bare" iconName="utility:success" alternativeText="success" size="x-small" />
      </span>
        <div class="slds-notify__content">
          <h2 class="slds-text-heading_small">{!$Label.c.AtualizadoSucesso}</h2>
        </div>
        <button class="slds-button slds-button_icon slds-notify__close slds-button_icon-inverse" title="Close" onclick="{!c.closeToastSuccess}">
          <lightning:icon variant="bare" iconName="utility:close" alternativeText="fechar" size="small" title="close" />
          <span class="slds-assistive-text">Close</span>
        </button>
      </div>
    </div>
  </div>
  <!-- fim do toast success -->

  <!-- toast warning -->
  <div class="demo-only slds-hide" aura:id="toastWarning" style="height: 4rem; position: fixed; width: 100%; z-index: 10000; top: 67px;">
    <div class="slds-notify_container slds-is-relative">
      <div class="slds-notify slds-notify_toast slds-theme--error" role="alert">
        <span class="slds-assistive-text">warning</span>
        <span class="slds-icon_container slds-icon-utility-info slds-m-right_small slds-no-flex slds-align-top" title="Description of icon when needed">
        <lightning:icon variant="bare" iconName="utility:error" alternativeText="error" size="small" />
      </span>
        <div class="slds-notify__content">
          <h2 class="slds-text-heading_small">{!$Label.c.ErroTenteNovamente}</h2>
        </div>
        <button class="slds-button slds-button_icon slds-notify__close slds-button_icon-inverse" title="Close" onclick="{!c.closeToastWarning}">
          <lightning:icon variant="bare" iconName="utility:close" alternativeText="fechar" size="x-small" title="close" />
          <span class="slds-assistive-text">Close</span>
        </button>
      </div>
    </div>
  </div>
  <!-- fim toast warning -->

  <!-- acoes -->
  <section>
    <ul class="lista-acoes">
      <li onclick="{!c.expandirTudo}">
        Expandir tudo
      </li>
      <li onclick="{!c.encolherTudo}">
        Encolher tudo
      </li>
    </ul>
  </section>
  <!-- fim acoes -->

  <!-- sessao precificacao -->
  <aura:if isTrue="{!v.precificacao.coberturas || v.precificacao.contratantes}">
    <section >
      <div class="slds-section" aura:id="section-action" >
        <h3 class="slds-section__title" >
          <button aria-controls="expando-unique-id" aria-expanded="true" class="slds-button slds-section__title-action" onclick="{!c.switchSection}">
            <lightning:icon class="slds-section__title-action-icon slds-button__icon_left" iconName="utility:switch" size="xx-small" alternativeText="Indica visibilidade da seção"/>
            <span class="slds-truncate" title="Section Title">Orçamento</span>
          </button>
        </h3>
        <div aria-hidden="false" class="slds-section__content" id="expando-unique-id">
          <aura:if isTrue="{!v.precificacao.coberturas}">
            <div class="content-overflow">
              <!-- Tabela com informacoes gerais -->
              <table class="slds-table slds-table_bordered slds-max-medium-table_stacked-horizontal slds-table_col-bordered slds-table_striped">
                <thead>
                <tr class="slds-text-title_caps">
                  <th scope="col">
                    <div class="slds-truncate" title=""></div>
                  </th>
                  <th scope="col">
                    <div class="slds-truncate" title="Capital">Capital </div>
                  </th>
                  <th scope="col">
                    <div class="slds-truncate" title="Taxa Pura (%)">Taxa <br/>Pura (%)</div>
                  </th>
                  <th scope="col">
                    <div class="slds-truncate" title="Taxa Comercial (%)">Taxa <br/>Comercial (%)</div>
                  </th>
                  <th scope="col">
                    <div class="slds-truncate" title="Taxa Total (%)">Taxa <br/>Total (%)</div>
                  </th>
                  <th scope="col">
                    <div class="slds-truncate" title="Prêmio Puro Calculado-Porto (R$)">Prêmio Puro <br/>Calculado-Porto (R$)</div>
                  </th>
                  <th scope="col">
                    <div class="slds-truncate" title="Prêmio Puro Calculado-Ressegurador (R$)">Prêmio Puro <br/>Calculado-Ressegurador (R$)</div>
                  </th>
                  <th scope="col">
                    <div class="slds-truncate" title="Prêmio Puro Calculado Total (R$)">Prêmio Puro <br/>Calculado Total (R$)</div>
                  </th>
                  <th scope="col">
                    <div class="slds-truncate" title="Prêmio Comercial (R$)">Prêmio <br/>Comercial (R$)</div>
                  </th>
                  <th scope="col">
                    <div class="slds-truncate" title="Prêmio Total (R$)">Prêmio <br/>Total (R$)</div>
                  </th>
                  <th scope="col">
                    <div class="slds-truncate" title="Desconto ou Agravo?">Desconto ou <br/>Agravo?</div>
                  </th>
                  <th scope="col">
                    <div class="slds-truncate" title="Valor (%)">Valor (%)</div>
                  </th>
                </tr>
                </thead>
                <tbody>
                <aura:iteration items="{!v.precificacao.coberturas}" var="cobertura" >
                  <tr data-id="{!cobertura.id}">
                    <th scope="row" data-label="Nome">
                      <div class="slds-truncate" title="Nome">
                        <ui:outputText value="{!cobertura.sigla}" />
                      </div>
                    </th>
                    <td data-label="Capital">
                      <div class="slds-truncate" title="Capital">
                        <ui:outputNumber value="{!cobertura.capital}" format="{!v.formatoMoeda}"/>
                      </div>
                    </td>
                    <td data-label="Taxa Pura (%)">
                      <div class="slds-truncate" title="Taxa Pura (%)">
                        <ui:outputNumber value="{!cobertura.taxa.pura}" format="{!v.formatoTaxa}"/>
                      </div>
                    </td>
                    <td data-label="Taxa Comercial (%)">
                      <div class="slds-truncate" title="Taxa Comercial (%)">
                        <ui:outputNumber value="{!cobertura.taxa.comercial}" format="{!v.formatoTaxa}"/>
                      </div>
                    </td>
                    <td data-label="Taxa Total (%)">
                      <div class="slds-truncate" title="Taxa Total (%)">
                        <ui:outputNumber value="{!cobertura.taxa.total}" format="{!v.formatoTaxa}"/>
                      </div>
                    </td>
                    <td data-label="Prêmio Puro Calculado - Porto (R$)">
                      <div class="slds-truncate" title="Prêmio Puro Calculado - Porto (R$)">
                        <ui:outputNumber value="{!cobertura.premio.puroInterno}" format="{!v.formatoMoeda}"/>
                      </div>
                    </td>
                    <td data-label="Prêmio Puro Calculado - Ressegurador (R$)">
                      <div class="slds-truncate" title="Prêmio Puro Calculado - Ressegurador (R$)">
                        <ui:outputNumber value="{!cobertura.premio.puroResseguro}" format="{!v.formatoMoeda}"/>
                      </div>
                    </td>
                    <td data-label="Prêmio Puro Calculado - Total (R$)">
                      <div class="slds-truncate" title="Prêmio Puro Calculado - Total (R$)">
                        <ui:outputNumber value="{!cobertura.premio.puro}" format="{!v.formatoMoeda}"/>
                      </div>
                    </td>
                    <td data-label="Prêmio Comercial (R$)">
                      <div class="slds-truncate" title="Prêmio Comercial (R$)">
                        <ui:outputNumber value="{!cobertura.premio.comercial}" format="{!v.formatoMoeda}"/>
                      </div>
                    </td>
                    <td data-label="Prêmio Total (R$)">
                      <div class="slds-truncate" title="Prêmio Total (R$)">
                        <ui:outputNumber value="{!cobertura.premio.total}" format="{!v.formatoMoeda}"/>
                      </div>
                    </td>
                    <td data-label="Desconto ou agravo?">
                      <div class="slds-truncate" >
                        <ui:inputSelect disabled="{! !v.ehEditavel }" class="single" aura:id="InputSelectSingle" value="{!cobertura.escolhaDescontoAgravo}" >
                          <ui:inputSelectOption text="" label="Escolha..."/>
                          <ui:inputSelectOption text="Desconto"/>
                          <ui:inputSelectOption text="Agravo"/>
                        </ui:inputSelect>
                      </div>
                    </td>
                    <td data-label="Valor (%)">
                      <div class="slds-truncate">
                        <lightning:input
                                disabled="{! !v.ehEditavel }"
                                variant="label-hidden"
                                type="number"
                                name="{!cobertura.idSf}"
                                value="{!cobertura.valorDescontoAgravo}"
                                formatter="percent-fixed"/>
                      </div>
                    </td>
                  </tr>
                </aura:iteration>

                <tr class="tr-total">
                  <th scope="row" data-label="Total">
                    <div class="slds-truncate" title="Total">
                      <ui:outputText value="Total" />
                    </div>
                  </th>
                  <td data-label="Capital">
                  </td>
                  <td data-label="Taxa Pura (%)">
                    <div class="slds-truncate" title="Taxa Pura (%)">
                      <ui:outputNumber value="{!v.precificacao.taxa.pura}" format="{!v.formatoTaxa}"/>
                    </div>
                  </td>
                  <td data-label="Taxa Comercial (%)">
                    <div class="slds-truncate" title="Taxa Comercial (%)">
                      <ui:outputNumber value="{!v.precificacao.taxa.comercial}" format="{!v.formatoTaxa}"/>
                    </div>
                  </td>
                  <td data-label="Taxa Total (%)">
                    <div class="slds-truncate" title="Taxa Total (%)">
                      <ui:outputNumber value="{!v.precificacao.taxa.total}" format="{!v.formatoTaxa}"/>
                    </div>
                  </td>
                  <td data-label="Prêmio Puro Calculado - Porto (R$)">
                    <div class="slds-truncate" title="Prêmio Puro Calculado - Porto (R$)">
                      <ui:outputNumber value="{!v.precificacao.premio.puroInterno}" format="{!v.formatoMoeda}"/>
                    </div>
                  </td>
                  <td data-label="Prêmio Puro Calculado - Ressegurador (R$)">
                    <div class="slds-truncate" title="Prêmio Puro Calculado - Ressegurador (R$)">
                      <ui:outputNumber value="{!v.precificacao.premio.puroResseguro}" format="{!v.formatoMoeda}"/>
                    </div>
                  </td>
                  <td data-label="Prêmio Puro Calculado - Total (R$)">
                    <div class="slds-truncate" title="Prêmio Puro Calculado - Total (R$)">
                      <ui:outputNumber value="{!v.precificacao.premio.puro}" format="{!v.formatoMoeda}"/>
                    </div>
                  </td>
                  <td data-label="Prêmio Comercial (R$)">
                    <div class="slds-truncate" title="Prêmio Comercial (R$)">
                      <ui:outputNumber value="{!v.precificacao.premio.comercial}" format="{!v.formatoMoeda}"/>
                    </div>
                  </td>
                  <td data-label="Prêmio Total (R$)">
                    <div class="slds-truncate" title="Prêmio Total (R$)">
                      <ui:outputNumber value="{!v.precificacao.premio.total}" format="{!v.formatoMoeda}"/>
                    </div>
                  </td>
                  <td data-label="Desconto ou agravo?">
                  </td>
                  <td data-label="Valor (%)">
                  </td>
                </tr>



                </tbody>
              </table>
              <!-- fim da tabela com informacoes gerais -->
            </div>
          </aura:if>

          <br/>

          <label class="slds-text-heading_medium">
            Prêmio informado:

          <ui:outputCurrency value="{!v.oportunidade.PremioInformado__c}" format="0.00000" />
          </label>

          <!-- Premio total: {! v.oportunidade.PremioTotal__c }
          Premio informado: {! v.oportunidade.PremioInformado__c } -->


          <aura:if isTrue="{!v.oportunidade.PremioInformado__c}">
            <aura:if isTrue="{! and((v.oportunidade.PremioInformado__c != null),(v.oportunidade.PremioTotal__c != v.oportunidade.PremioInformado__c) )}">
              <h2 style="display: inline-block; margin-left: 30px;" class=" slds-text-heading_medium slds-text-color_error">Há divergência de prêmio</h2>
            </aura:if>
          </aura:if>

          <br/>

          <!-- Secao por segurado -->
          <aura:if isTrue="{!v.precificacao.contratantes}" >
            <h2 class="slds-section__title">Contratantes</h2>
            <aura:iteration var="contratante" items="{!v.precificacao.contratantes}" indexVar="index">
              <section >
                <div class="slds-section" aura:id="section-action" >
                  <h3 class="slds-section__title" >
                    <button aria-controls="expando-unique-id" aria-expanded="true" class="slds-button slds-section__title-action section-child-1" onclick="{!c.switchSection}">
                      <lightning:icon class="slds-section__title-action-icon slds-button__icon_left" iconName="utility:switch" size="xx-small" alternativeText="Indica visibilidade da seção"/>
                      <span class="slds-truncate" title="Section Title">{!contratante.pessoa.nome}</span>
                    </button>
                  </h3>
                  <div aria-hidden="false" class="slds-section__content content-p-b" id="expando-unique-id">
                    <aura:if isTrue="{!contratante.coberturas}">
                      <div class="content-overflow">
                        <!-- Tabela com informacoes gerais -->
                        <table class="slds-table slds-table_bordered slds-max-medium-table_stacked-horizontal slds-table_col-bordered slds-table_striped">
                          <thead>
                          <tr class="slds-text-title_caps">
                            <th scope="col">
                              <div class="slds-truncate" title=""></div>
                            </th>
                            <th scope="col">
                              <div class="slds-truncate" title="Capital">Capital </div>
                            </th>
                            <th scope="col">
                              <div class="slds-truncate" title="Taxa Pura (%)">Taxa <br/>Pura (%)</div>
                            </th>
                            <th scope="col">
                              <div class="slds-truncate" title="Taxa Comercial (%)">Taxa <br/>Comercial (%)</div>
                            </th>
                            <th scope="col">
                              <div class="slds-truncate" title="Taxa Total (%)">Taxa <br/>Total (%)</div>
                            </th>
                            <th scope="col">
                              <div class="slds-truncate" title="Prêmio Comercial (R$)">Prêmio <br/>Puro (R$)</div>
                            </th>
                            <th scope="col">
                              <div class="slds-truncate" title="Prêmio Puro Calculado-Porto (R$)">Prêmio <br/>Comercial (R$)</div>
                            </th>
                            <th scope="col">
                              <div class="slds-truncate" title="Prêmio Total (R$)">Prêmio <br/>Total (R$)</div>
                            </th>
                          </tr>
                          </thead>
                          <tbody>
                          <aura:iteration items="{!contratante.coberturas}" var="cobertura" >
                            <tr>
                              <th scope="row" data-label="Nome">
                                <div class="slds-truncate" title="Nome">
                                  <ui:outputText value="{!cobertura.sigla}" />
                                </div>
                              </th>
                              <td data-label="Capital">
                                <div class="slds-truncate" title="Capital">
                                  <ui:outputNumber value="{!cobertura.capital}" format="{!v.formatoMoeda}"/>
                                </div>
                              </td>
                              <td data-label="Taxa Pura (%)">
                                <div class="slds-truncate" title="Taxa Pura (%)">
                                  <ui:outputNumber value="{!cobertura.taxa.pura}" format="{!v.formatoTaxa}"/>
                                </div>
                              </td>
                              <td data-label="Taxa Comercial (%)">
                                <div class="slds-truncate" title="Taxa Comercial (%)">
                                  <ui:outputNumber value="{!cobertura.taxa.comercial}" format="{!v.formatoTaxa}"/>
                                </div>
                              </td>
                              <td data-label="Taxa Total (%)">
                                <div class="slds-truncate" title="Taxa Total (%)">
                                  <ui:outputNumber value="{!cobertura.taxa.total}" format="{!v.formatoTaxa}"/>
                                </div>
                              </td>
                              <td data-label="Prêmio Comercial (R$)">
                                <div class="slds-truncate" title="Prêmio Puro (R$)">
                                  <ui:outputNumber value="{!cobertura.premio.puro}" format="{!v.formatoMoeda}"/>
                                </div>
                              </td>
                              <td data-label="Prêmio Puro (R$)">
                                <div class="slds-truncate" title="Prêmio Comercial (R$)">
                                  <ui:outputNumber value="{!cobertura.premio.comercial}" format="{!v.formatoMoeda}"/>
                                </div>
                              </td>
                              <td data-label="Prêmio Total (R$)">
                                <div class="slds-truncate" title="Prêmio Total (R$)">
                                  <ui:outputNumber value="{!cobertura.premio.total}" format="{!v.formatoMoeda}"/>
                                </div>
                              </td>
                            </tr>
                          </aura:iteration>
                          </tbody>
                        </table>
                        <!-- Fim da tabela com informacoes gerais -->
                      </div>
                    </aura:if>
                    <!-- Secao grupos do contratante -->
                    <aura:if isTrue="{!contratante.grupos}">
                      <h2 class="slds-section__title">Grupos</h2>
                      <aura:iteration items="{!contratante.grupos}" var="grupo">
                        <section >
                          <div class="slds-section" aura:id="section-action" >
                            <h3 class="slds-section__title" >
                              <button aria-controls="expando-unique-id" aria-expanded="true" class="slds-button slds-section__title-action section-child-2" onclick="{!c.switchSection}">
                                <lightning:icon class="slds-section__title-action-icon slds-button__icon_left" iconName="utility:switch" size="xx-small" alternativeText="Indica visibilidade da seção"/>
                                <span class="slds-truncate" title="Section Title">{!contratante.pessoa.nome + ' - ' + grupo.nome}</span>
                              </button>
                            </h3>
                            <div aria-hidden="false" class="slds-section__content" id="expando-unique-id">
                              <div class="content-overflow">
                                <table class="slds-table slds-table_bordered slds-max-medium-table_stacked-horizontal slds-table_col-bordered slds-table_striped m-b-10">
                                  <thead>
                                  <tr class="slds-text-title_caps">
                                    <th scope="col">
                                      <div class="slds-truncate" title="Desconto ou Agravo?">Desconto ou Agravo?</div>
                                    </th>
                                    <th scope="col">
                                      <div class="slds-truncate" title="Valor (%)">Valor (%)</div>
                                    </th>
                                  </tr>
                                  </thead>
                                  <tbody>
                                  <tr>
                                    <td data-label="Desconto ou Agravo no faturamento?">
                                      <div class="slds-truncate" title="Desconto ou Agravo no faturamento?">
                                        <ui:inputSelect disabled="{! !v.ehEditavel }" class="single" aura:id="InputSelectSingle" value="{!grupo.escolhaDescontoAgravo}">
                                          <ui:inputSelectOption text="" label="Escolha..."/>
                                          <ui:inputSelectOption text="Desconto"/>
                                          <ui:inputSelectOption text="Agravo"/>
                                        </ui:inputSelect>
                                      </div>
                                    </td>
                                    <td data-label="Valor (%)">
                                      <div class="slds-truncate" title="Valor (%)">
                                        <lightning:input
                                                disabled="{! !v.ehEditavel }"
                                                variant="label-hidden"
                                                type="number"
                                                name="{!grupo.idSf}"
                                                value="{!grupo.valorDescontoAgravo}"
                                                formatter="percent-fixed"/>
                                      </div>
                                    </td>
                                  </tr>
                                  </tbody>
                                </table>
                              </div>
                              <div class="content-overflow">
                                <!-- tabela com as informacoes por grupo do contratante -->
                                <table class="slds-table slds-table_bordered slds-max-medium-table_stacked-horizontal slds-table_col-bordered slds-table_striped">
                                  <thead>
                                  <tr class="slds-text-title_caps">
                                    <th scope="col">
                                      <div class="slds-truncate" title=""></div>
                                    </th>
                                    <th scope="col">
                                      <div class="slds-truncate" title="Capital">Capital </div>
                                    </th>
                                    <th scope="col">
                                      <div class="slds-truncate" title="Taxa Pura (%)">Taxa <br/>Pura (%)</div>
                                    </th>
                                    <th scope="col">
                                      <div class="slds-truncate" title="Taxa Comercial (%)">Taxa <br/>Comercial (%)</div>
                                    </th>
                                    <th scope="col">
                                      <div class="slds-truncate" title="Taxa Total (%)">Taxa <br/>Total (%)</div>
                                    </th>
                                    <th scope="col">
                                      <div class="slds-truncate" title="Prêmio Comercial (R$)">Prêmio <br/>Puro (R$)</div>
                                    </th>
                                    <th scope="col">
                                      <div class="slds-truncate" title="Prêmio Puro Calculado-Porto (R$)">Prêmio <br/>Comercial (R$)</div>
                                    </th>
                                    <th scope="col">
                                      <div class="slds-truncate" title="Prêmio Total (R$)">Prêmio <br/>Total (R$)</div>
                                    </th>
                                    <th scope="col">
                                      <div class="slds-truncate" title="Desconto ou Agravo?">Desconto ou <br/>Agravo?</div>
                                    </th>
                                    <th scope="col">
                                      <div class="slds-truncate" title="Valor (%)">Valor (%)</div>
                                    </th>
                                  </tr>
                                  </thead>
                                  <tbody>
                                  <aura:iteration items="{!grupo.coberturas}" var="cobertura" >
                                    <tr>
                                      <th scope="row" data-label="Nome">
                                        <div class="slds-truncate" title="Nome">
                                          <ui:outputText value="{!cobertura.sigla}" />
                                        </div>
                                      </th>
                                      <td data-label="Capital">
                                        <div class="slds-truncate" title="Capital">
                                          <ui:outputNumber value="{!cobertura.capital}" format="{!v.formatoMoeda}"/>
                                        </div>
                                      </td>
