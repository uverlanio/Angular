<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IndicadorLiberacao__c</fullName>
    <description>Indica se o registro corrente apresenta ou não critica pendente</description>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL(Status__c, &apos;Liberado&apos;) , 0, 1)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Indicador Liberação</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
