<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ValidacaoDataFinalPendencia</fullName>
    <active>true</active>
    <errorConditionFormula>DataInicioPendencia__c &gt;  DataFimPendencia__c</errorConditionFormula>
    <errorMessage>Data Final da Pendência não pode ser menor do que a data inicial da Pendência</errorMessage>
</ValidationRule>
