<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ValidacaoDataInicialPendencia</fullName>
    <active>true</active>
    <errorConditionFormula>DataInicioAnalise__c &gt;  DataInicioPendencia__c</errorConditionFormula>
