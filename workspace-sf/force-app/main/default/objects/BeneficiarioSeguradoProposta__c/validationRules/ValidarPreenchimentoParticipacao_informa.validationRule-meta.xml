<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ValidarPreenchimentoParticipacao_informa</fullName>
    <active>true</active>
    <errorConditionFormula>IF(ISPICKVAL(TipoBeneficiario__c, &quot;Nome do beneficiário&quot;) &amp;&amp;  ISNULL(Participacao__c ) , true, false)</errorConditionFormula>
