<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ValidarRotuloNome</fullName>
    <active>true</active>
    <errorConditionFormula>MasterLabel != DeveloperName</errorConditionFormula>
    <errorDisplayField>DeveloperName</errorDisplayField>
    <errorMessage>O rótulo e o Nome da Tradução devem ser iguais!</errorMessage>
</ValidationRule>
