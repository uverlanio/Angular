<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ImpedeAlterarRemuneracaoObrigatoria</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
ISCHANGED(RemuneracaoProduto__c ),
PRIORVALUE( Obrigatoria__c ))</errorConditionFormula>
    <errorDisplayField>RemuneracaoProduto__c</errorDisplayField>
    <errorMessage>Remuneração obrigatória não pode ser alterada.</errorMessage>
</ValidationRule>
