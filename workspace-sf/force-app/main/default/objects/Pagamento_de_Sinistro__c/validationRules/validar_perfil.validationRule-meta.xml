<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>validar_perfil</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(OR(ISNEW(), $Profile.Name == &apos;API&apos;, $Profile.Name = &apos;Administrador do sistema&apos;))</errorConditionFormula>
    <errorMessage>Você não tem perfil para alterar ou criar este pagamento de sinistro</errorMessage>
</ValidationRule>
