<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ValidarLimiteMaximoUtilizacao</fullName>
    <active>true</active>
    <errorConditionFormula>AND( LimiteMaximoUtilizacao__c &gt; 365 )</errorConditionFormula>
    <errorDisplayField>LimiteMaximoUtilizacao__c</errorDisplayField>
    <errorMessage>O limite máximo de utilização é de 365.</errorMessage>
</ValidationRule>
