<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ValidarDependentesCancelar</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
    CancelaDependentes__c,
    ISBLANK(DependentesCancelar__c) 
)</errorConditionFormula>
    <errorMessage>Preencha o campo Dependentes para cancelar</errorMessage>
</ValidationRule>
