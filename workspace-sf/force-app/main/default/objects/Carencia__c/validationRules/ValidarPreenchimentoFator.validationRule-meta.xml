<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ValidarPreenchimentoFator</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
    NOT(ISBLANK(LimitePercentual__c)),
    NOT(ISBLANK(LimiteValor__c))
)</errorConditionFormula>
    <errorMessage>Preencha somente um fator</errorMessage>
</ValidationRule>
