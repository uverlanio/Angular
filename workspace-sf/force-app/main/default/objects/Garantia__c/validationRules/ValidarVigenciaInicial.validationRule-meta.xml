<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ValidarVigenciaInicial</fullName>
    <active>true</active>
    <errorConditionFormula>VigenciaInicial__c &lt; TODAY()</errorConditionFormula>
    <errorMessage>A vigência não pode ser inferior a data atual</errorMessage>
</ValidationRule>
