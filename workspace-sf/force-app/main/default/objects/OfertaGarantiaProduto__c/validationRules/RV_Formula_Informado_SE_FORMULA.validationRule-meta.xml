<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>RV_Formula_Informado_SE_FORMULA</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    ISPICKVAL(TipoCalculo__c,&quot;Fórmula&quot;),
				ISBLANK(Formula__c)
)</errorConditionFormula>
    <errorMessage>Fórmula (Cálculo do Valor do Capital) deve ser preenchido!</errorMessage>
</ValidationRule>
