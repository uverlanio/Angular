<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ValidarEdicaoQuestionario</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
    NOT(ISPICKVAL(Status__c, &apos;Em manutenção&apos;)),
    NOT(ISCHANGED(Status__c))
)</errorConditionFormula>
    <errorMessage>Os questionário somente poderão ser editados em manutenção</errorMessage>
</ValidationRule>
